{
  "name": "nyco/working-nyc",
  "description": "Find and connect to services and resources that can help you prepare for and get a job.",
  "version": "1.3.0",
  "authors": [
    {
      "name": "NYC Opportunity",
      "email": "products@nycopportunity.nyc.gov",
      "homepage": "http://nyc.gov/opportunity"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "nyco/wp-assets": "^1.1",
    "nyco/wp-config": "^1.0",
    "spatie/schema-org": "^2.8",
    "timber/timber": "^1.16",
    "wpackagist-plugin/acf-to-rest-api": "^3.3",
    "wpackagist-plugin/aryo-activity-log": "^2.5",
    "wpackagist-plugin/custom-post-type-ui": "^1.7",
    "wpackagist-plugin/duplicate-post": "^3.2",
    "wpackagist-plugin/gathercontent-import": "^3.1",
    "wpackagist-plugin/google-authenticator": "^0.52.0",
    "wpackagist-plugin/limit-login-attempts-reloaded": "^2.9",
    "wpackagist-plugin/loggedin": "^1.2",
    "wpackagist-plugin/rollbar": "^2.6",
    "wpackagist-plugin/simple-custom-post-order": "^2.5",
    "wpackagist-plugin/taxonomy-terms-order": "^1.5",
    "wpackagist-plugin/wordpress-importer": "^0.7.0",
    "wpackagist-plugin/wp-all-export": "^1.2",
    "wpackagist-plugin/wp-all-import": "^3.5",
    "wpackagist-plugin/wp-security-questions": "^1.0",
    "wpackagist-plugin/wps-hide-login": "^1.5.2.1",
    "wpackagist-plugin/wpscan": "^1.15"
  },
  "require-dev": {
    "filp/whoops": "*",
    "nielsdeblaauw/query-monitor-twig-profile": "*",
    "squizlabs/php_codesniffer": "*",
    "whoops/soap": "*",
    "wpackagist-plugin/query-monitor": "*",
    "wpackagist-plugin/redis-cache": "*",
    "wpackagist-plugin/wp-auto-login": "*",
    "wpackagist-plugin/wp-crontrol": "*"
  },
  "scripts": {
    "development": [
      "composer dump-autoload"
    ],
    "production": [
      "composer dump-autoload --no-dev --optimize"
    ],
    "predeploy": [
      "@development",
      "@lint",
      "@production"
    ],
    "lint": [
      "vendor/bin/phpcs --standard=phpcs.xml"
    ],
    "fix": [
      "vendor/bin/phpcbf --standard=phpcs.xml"
    ],
    "version": [
      "composer update nothing",
      "@production"
    ],
    "deps": [
      "composer show --tree"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
